	summary := OrderedCollection new.
	transactions do: [:transaction |	
			(transaction isKindOf: Deposit) 		ifTrue: [summary add: ('Depsito por ', transaction value asString , '.')].			
			(transaction isKindOf: Withdraw ) 		ifTrue: [summary add: 'Extraccin por ', transaction value asString , '.'].		
